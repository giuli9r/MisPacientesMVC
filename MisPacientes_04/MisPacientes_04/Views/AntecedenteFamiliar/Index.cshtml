@model IEnumerable<MisPacientes_04.Models.AntecedenteFamiliar>

@{
    ViewData["Title"] = "Index";
}

<div class="alert alert-dark fs-1 text" role="alert">
  Antecedentes Familiares
</div>



<p>
    @* <a asp-action="Create">Create New</a> *@
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.E_Cardios)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.E_Respiratorias)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DescripcionAntecedentes)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Anamnesis)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.E_Cardios)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.E_Respiratorias)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DescripcionAntecedentes)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Anamnesis.PacienteNombreAnamnesis)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
